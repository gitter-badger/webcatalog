{
  "name": "WebCatalog",
  "version": "2.8.2",
  "description": "WebCatalog, run your favorite web apps natively",
  "main": "main.js",
  "scripts": {
    "start": "electron ./app",
    "dev": "cross-env NODE_ENV=development webpack -d --watch",
    "compile": "cross-env NODE_ENV=production webpack -p",
    "package": "node ./packager.js",
    "lint": "eslint ./src --ext .js --cache",
    "dist": "npm run compile && npm run package"
  },
  "repository": "https://github.com/webcatalog/desktop",
  "author": {
    "name": "Quang Lam",
    "email": "quang.lam2807@gmail.com",
    "url": "https://quanglam.me"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "electron": "^1.4.14",
    "electron-builder": "11.3.0",
    "electron-builder-squirrel-windows": "^11.4.0",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "webpack": "2.2.0",
    "webpack-merge": "^2.4.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.6.0",
    "algoliasearch": "^3.20.4",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-batched-actions": "^0.1.5",
    "redux-thunk": "^2.1.0"
  }
}
